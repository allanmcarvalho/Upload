{
    "name": "allanmcarvalho/upload",
    "description": "Upload plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": ">=3.4.0 <4.0.0",
        "intervention/image": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Upload\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Upload\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
